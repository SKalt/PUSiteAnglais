$(document).ready(function(){
	$("#menuCS1titre").removeClass("menurechercheunselect");
	$("#menuCS1titre").addClass("menurechercheselect");
	$("#CSgen_0").removeClass("menurechercheoff");
	$("#CSgen_0").addClass("menurechercheon");
    $("#CSgen_0").click(function(){
		$("#CScomportement").slideUp("fast") ;
		$("#CScomportement_0").addClass("menurechercheoff");
		$("#CScomportement_0").removeClass("menurechercheon");
		$("#CSsecu").slideUp("fast");
		$("#CSsecu_0").addClass("menurechercheoff");
		$("#CSsecu_0").removeClass("menurechercheon");
		$("#CSmission").slideUp("fast");
		$("#CSmission_0").addClass("menurechercheoff");
		$("#CSmission_0").removeClass("menurechercheon");
        $("#CSetapes").slideUp("fast");
		$("#CSetapes_0").addClass("menurechercheoff");
		$("#CSetapes_0").removeClass("menurechercheon");
		//$("#CSdocuments").slideUp("fast");
		//$("#CSdocuments_0").addClass("menurechercheoff");
		//$("#CSdocuments_0").removeClass("menurechercheon");
		$("#CSequipe").slideUp("fast");
		$("#CSequipe_0").addClass("menurechercheoff");
		$("#CSequipe_0").removeClass("menurechercheon");
		
        $("#CSgen").slideDown("slow");
		$("#CSgen_0").addClass("menurechercheon");
		$("#CSgen_0").removeClass("menurechercheoff");
		return(false) ;
    });
});

